/* disabled widget */
QWidget:disabled
{
    color: #525252;
    background-color: #323232;
}

/* table items list */
QAbstractItemView
{
    background-color: #444444;
}

/* edit line clicked */
QWidget:focus
{
    border: 1px solid #258bdd;
}

/* edit line */
QLineEdit
{
    background-color: #444444;
    padding: 1px;
    border-style: solid;
    border: 1px solid #444444;
    border-radius: 2;
}


/* button */
QPushButton
{
    color: #b1b1b1;
    background-color: #343434;
    border-width: 1px;
    border-color: #1e1e1e;
    border-style: solid;
    border-radius: 2px;
    padding: 3px;
    font-size: 12px;
    padding-left: 5px;
    padding-right: 5px;
}

/* button pressed */
QPushButton:pressed
{
    background-color: #258bdd;
}


/* combobox main */
QComboBox
{
    selection-background-color: #258bdd;
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #565656, stop: 0.1 #525252, stop: 0.5 #4e4e4e, stop: 0.9 #4a4a4a, stop: 1 #464646);
    border-style: solid;
    border: 1px solid #258bdd;
    border-radius: 2;
}

/* combobox and pushbutton hover effect */
QComboBox:hover,QPushButton:hover
{
    border: 2px solid #258bdd;
}


/* combobox selected element */
QComboBox:on
{
    padding-top: 3px;
    padding-left: 4px;
    background-color: #343434;
    selection-background-color: #258bdd;
}

/* combobox items */
QComboBox QAbstractItemView
{
    border: 2px solid darkgray;
    selection-background-color: #258bdd;
}


QComboBox::drop-down
{
     subcontrol-origin: padding;
     subcontrol-position: top right;
     width: 15px;

     border-left-width: 0px;
     border-left-color: darkgray;
     border-left-style: solid; /* just a single line */
     border-top-right-radius: 2px; /* same radius as the QComboBox */
     border-bottom-right-radius: 2px;
 }

QComboBox::down-arrow
{
     image: url(:/images/images/down_arrow.png);
}

/* groupbox */
QGroupBox {
    padding-top: 10px;
    border: 1px solid #444444;
}

/* groupbox clicked */
QGroupBox:focus
{
    border: 1px solid #258bdd;
}


QTextEdit:focus
{
    border: 1px solid #258bdd;
}



























/* ---------------------------------------------------------------------------------------------------------------------------------- */

/* horizontal scroll line background */
QScrollBar:horizontal {
     /* border: 1px solid #222222; */
     background: #343434;
     height: 12px;
     margin: 0px 16px 0 16px;
}

/* horizontal scroll line handle */
QScrollBar::handle:horizontal
{
      background: #258bdd;
      min-height: 20px;
      border-radius: 2px;
}

/* Right arrow horizontal scroll handle */
QScrollBar::add-line:horizontal {
      border: 1px solid #258bdd;
      border-radius: 2px;
      background: #258bdd;
      width: 14px;
      subcontrol-position: right;
      subcontrol-origin: margin;
}

/* Left arrow horizontal scroll handle*/
QScrollBar::sub-line:horizontal {
      border: 1px solid #258bdd;
      border-radius: 2px;
      background: #258bdd;
      width: 14px;
     subcontrol-position: left;
     subcontrol-origin: margin;
}

/* horizontal scroll arrows */
QScrollBar::right-arrow:horizontal, QScrollBar::left-arrow:horizontal
{
      border: 1px solid black;
      width: 1px;
      height: 1px;
      background: white;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
      background: none;
}








/* vertical scroll line background */
QScrollBar:vertical {
     /* border: 1px solid #222222; */
     background: #343434;
     height: 12px;
     margin: 0px 16px 0 16px;
}

/* vertical scroll line handle */
QScrollBar::handle:vertical
{
      background: #258bdd;
      min-height: 20px;
      border-radius: 2px;
}

/* Right arrow vertical scroll handle */
QScrollBar::add-line:vertical {
      border: 1px solid #258bdd;
      border-radius: 2px;
      background: #258bdd;
      width: 14px;
      subcontrol-position: right;
      subcontrol-origin: margin;
}

/* Left arrow vertical scroll handle*/
QScrollBar::sub-line:vertical {
      border: 1px solid #258bdd;
      border-radius: 2px;
      background: #258bdd;
      width: 14px;
     subcontrol-position: left;
     subcontrol-origin: margin;
}

/* vertical scroll arrows */
QScrollBar::right-arrow:vertical, QScrollBar::left-arrow:vertical
{
      border: 1px solid black;
      width: 1px;
      height: 1px;
      background: white;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
      background: none;
}



